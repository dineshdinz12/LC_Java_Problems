class Solution {
    public String largestGoodInteger(String num) {
        String largest = "";
        for (int i = 0; i <= num.length() - 3; i++) {
            char c = num.charAt(i);
            if (c == num.charAt(i + 1) && c == num.charAt(i + 2)) {
                String sub = num.substring(i, i + 3);
                if (sub.compareTo(largest) > 0) { 
                    largest = sub;
                }
            }
        }
        return largest;
    }
}
